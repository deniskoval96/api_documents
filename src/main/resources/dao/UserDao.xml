<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hackathon.api.dao.UserDao">

    <select id="getAllUsers" resultType="com.hackathon.api.domain.User">
        SELECT
            u.id AS id,
            u.name AS name,
            u.surname AS surname
        FROM user u
    </select>

</mapper>